<?xml version="1.0" encoding="UTF-8"?>
<document type="com.apple.InterfaceBuilder3.Cocoa.XIB" version="3.0" toolsVersion="19455" targetRuntime="MacOSX.Cocoa" propertyAccessControl="none" useAutolayout="YES">
    <dependencies>
        <deployment identifier="macosx"/>
        <plugIn identifier="com.apple.InterfaceBuilder.CocoaPlugin" version="19455"/>
        <capability name="documents saved in the Xcode 8 format" minToolsVersion="8.0"/>
    </dependencies>
    <objects>
        <customObject id="-2" userLabel="File's Owner" customClass="PreferencesWindowController">
            <connections>
                <outlet property="window" destination="1" id="3"/>
                <outlet property="modeMatrix" destination="mode-matrix" id="mode-matrix-outlet"/>
                <outlet property="intervalPopup" destination="interval-popup" id="interval-popup-outlet"/>
                <outlet property="statusLabel" destination="status-label" id="status-label-outlet"/>
            </connections>
        </customObject>
        <customObject id="-1" userLabel="First Responder" customClass="FirstResponder"/>
        <customObject id="-3" userLabel="Application" customClass="NSObject"/>
        <window title="Mac Audio Keepalive - Preferences" allowsToolTipsWhenApplicationIsInactive="NO" autorecalculatesKeyViewLoop="NO" releasedWhenClosed="NO" visibleAtLaunch="NO" frameAutosaveName="" animationBehavior="default" id="1" customClass="NSWindow">
            <windowStyleMask key="styleMask" titled="YES" closable="YES" miniaturizable="NO" resizable="NO"/>
            <rect key="contentRect" x="0" y="0" width="480" height="320"/>
            <rect key="screenRect" x="0" y="0" width="1920" height="1080"/>
            <view key="contentView" id="2">
                <rect key="frame" x="0" y="0" width="480" height="320"/>
                <autoresizingMask key="autoresizingMask"/>
                <subviews>
                    <!-- Audio Mode Section -->
                    <textField verticalHuggingPriority="750" horizontalCompressionResistancePriority="250" id="mode-label">
                        <rect key="frame" x="20" y="270" width="440" height="17"/>
                        <autoresizingMask key="autoresizingMask" flexibleMaxX="YES" flexibleMinY="YES"/>
                        <textFieldCell key="cell" lineBreakMode="clipping" title="Audio Mode:" id="mode-label-cell">
                            <font key="font" metaFont="systemBold" size="13"/>
                            <color key="textColor" name="labelColor" catalog="System" colorSpace="catalog"/>
                            <color key="backgroundColor" name="textBackgroundColor" catalog="System" colorSpace="catalog"/>
                        </textFieldCell>
                    </textField>

                    <!-- Mode Radio Buttons Matrix -->
                    <matrix verticalHuggingPriority="750" allowsEmptySelection="NO" autosizesCells="NO" id="mode-matrix">
                        <rect key="frame" x="20" y="180" width="440" height="80"/>
                        <autoresizingMask key="autoresizingMask" flexibleMaxX="YES" flexibleMinY="YES"/>
                        <color key="backgroundColor" name="controlColor" catalog="System" colorSpace="catalog"/>
                        <size key="cellSize" width="440" height="40"/>
                        <size key="intercellSpacing" width="4" height="2"/>
                        <buttonCell key="prototype" type="radio" title="Radio" imagePosition="left" alignment="left" lineBreakMode="truncatingTail" state="on" borderStyle="border" imageScaling="proportionallyDown" inset="2" id="mode-cell-prototype">
                            <behavior key="behavior" changeContents="YES" doesNotDimImage="YES" lightByContents="YES"/>
                            <font key="font" metaFont="system"/>
                        </buttonCell>
                        <cells>
                            <column>
                                <buttonCell type="radio" title="Continuous Mode (prevents HDMI/optical dropouts)" imagePosition="left" alignment="left" tag="0" id="mode-continuous">
                                    <behavior key="behavior" changeContents="YES" doesNotDimImage="YES" lightByContents="YES"/>
                                    <font key="font" metaFont="system"/>
                                </buttonCell>
                                <buttonCell type="radio" title="Interval Mode (prevents speaker standby with periodic pulses)" imagePosition="left" alignment="left" tag="1" id="mode-interval">
                                    <behavior key="behavior" changeContents="YES" doesNotDimImage="YES" lightByContents="YES"/>
                                    <font key="font" metaFont="system"/>
                                </buttonCell>
                            </column>
                        </cells>
                        <connections>
                            <action selector="modeChanged:" target="-2" id="mode-action"/>
                        </connections>
                    </matrix>

                    <!-- Interval Settings Section -->
                    <textField verticalHuggingPriority="750" horizontalCompressionResistancePriority="250" id="interval-label">
                        <rect key="frame" x="20" y="145" width="440" height="17"/>
                        <autoresizingMask key="autoresizingMask" flexibleMaxX="YES" flexibleMinY="YES"/>
                        <textFieldCell key="cell" lineBreakMode="clipping" title="Interval Settings:" id="interval-label-cell">
                            <font key="font" metaFont="systemBold" size="13"/>
                            <color key="textColor" name="labelColor" catalog="System" colorSpace="catalog"/>
                            <color key="backgroundColor" name="textBackgroundColor" catalog="System" colorSpace="catalog"/>
                        </textFieldCell>
                    </textField>

                    <textField verticalHuggingPriority="750" horizontalCompressionResistancePriority="250" id="interval-prompt">
                        <rect key="frame" x="20" y="115" width="140" height="17"/>
                        <autoresizingMask key="autoresizingMask" flexibleMaxX="YES" flexibleMinY="YES"/>
                        <textFieldCell key="cell" lineBreakMode="clipping" alignment="right" title="Play pulse every:" id="interval-prompt-cell">
                            <font key="font" metaFont="system"/>
                            <color key="textColor" name="labelColor" catalog="System" colorSpace="catalog"/>
                            <color key="backgroundColor" name="textBackgroundColor" catalog="System" colorSpace="catalog"/>
                        </textFieldCell>
                    </textField>

                    <popUpButton verticalHuggingPriority="750" id="interval-popup">
                        <rect key="frame" x="165" y="110" width="120" height="25"/>
                        <autoresizingMask key="autoresizingMask" flexibleMaxX="YES" flexibleMinY="YES"/>
                        <popUpButtonCell key="cell" type="push" title="25 minutes" bezelStyle="rounded" alignment="left" lineBreakMode="truncatingTail" state="on" borderStyle="borderAndBezel" tag="25" imageScaling="proportionallyDown" inset="2" selectedItem="interval-25" id="interval-popup-cell">
                            <behavior key="behavior" lightByBackground="YES" lightByGray="YES"/>
                            <font key="font" metaFont="menu"/>
                            <menu key="menu" title="OtherViews" id="interval-menu">
                                <items>
                                    <menuItem title="5 minutes" tag="5" id="interval-5"/>
                                    <menuItem title="10 minutes" tag="10" id="interval-10"/>
                                    <menuItem title="15 minutes" tag="15" id="interval-15"/>
                                    <menuItem title="20 minutes" tag="20" id="interval-20"/>
                                    <menuItem title="25 minutes" state="on" tag="25" id="interval-25"/>
                                    <menuItem title="30 minutes" tag="30" id="interval-30"/>
                                </items>
                            </menu>
                        </popUpButtonCell>
                        <connections>
                            <action selector="intervalChanged:" target="-2" id="interval-action"/>
                        </connections>
                    </popUpButton>

                    <!-- Status Section -->
                    <textField verticalHuggingPriority="750" horizontalCompressionResistancePriority="250" id="status-label">
                        <rect key="frame" x="20" y="70" width="440" height="17"/>
                        <autoresizingMask key="autoresizingMask" flexibleMaxX="YES" flexibleMinY="YES"/>
                        <textFieldCell key="cell" lineBreakMode="clipping" title="âœ“ Audio keepalive is active" id="status-label-cell">
                            <font key="font" metaFont="system"/>
                            <color key="textColor" name="secondaryLabelColor" catalog="System" colorSpace="catalog"/>
                            <color key="backgroundColor" name="textBackgroundColor" catalog="System" colorSpace="catalog"/>
                        </textFieldCell>
                    </textField>

                    <!-- Reset Button -->
                    <button verticalHuggingPriority="750" id="reset-button">
                        <rect key="frame" x="315" y="20" width="150" height="32"/>
                        <autoresizingMask key="autoresizingMask" flexibleMaxX="YES" flexibleMinY="YES"/>
                        <buttonCell key="cell" type="push" title="Reset to Defaults" bezelStyle="rounded" alignment="center" borderStyle="border" imageScaling="proportionallyDown" inset="2" id="reset-button-cell">
                            <behavior key="behavior" pushIn="YES" lightByBackground="YES" lightByGray="YES"/>
                            <font key="font" metaFont="system"/>
                        </buttonCell>
                        <connections>
                            <action selector="resetToDefaults:" target="-2" id="reset-action"/>
                        </connections>
                    </button>
                </subviews>
            </view>
            <connections>
                <outlet property="delegate" destination="-2" id="4"/>
            </connections>
        </window>
    </objects>
</document>
